    deactivate 0
    activate a
    a --> b
    deactivate a
    activate b
    b --> d:1.1:正常終了時()
    activate d
    d --> e
    activate e
    deactivate e
    deactivate d
    b --> f:1.2:入力間違い()
    activate f
    f --> g
    activate g
    activate c
    b --> c
    c --> b
    deactivate c 
@enduml

